<Theme HexStyle="100a0000">
    <Application Width="400" Height="200" FontId="0">Standard Bootstrapper Application</Application>
    <Font id="0" Height="-16" Weight="700" Foreground="0" Background="ffffff">Consolas</Font>
    <Font id="1" Height="-16" Weight="700" Foreground="0" Background="ffffff" Underline="yes">Consolas</Font>
    <Font id="2" Height="-16" Weight="700" Foreground="ff0000" Background="ffffff" Underline="no">Consolas</Font>
    <Font id="3" Height="-16" Weight="700" Foreground="00ff00" Background="ffffff" Underline="yes">Consolas</Font>
    <Font id="4" Height="-16" Weight="700" Foreground="0">Consolas</Font>
    <Button Name="MinimizeButton" X="-30" Y="5" Width="12" Height="12" FontId="0">_</Button>
    <Button Name="CloseButton" X="-15" Y="5" Width="12" Height="12" FontId="0">X</Button>
    <Page Name="Install">
        <Button Name="InstallButton" X="-80" Y="-10" Width="80" Height="24" FontId="0">Install</Button>
        <Button Name="OptionsButton" X="-170" Y="-10" Width="80" Height="24" FontId="0">Options</Button>
        <Richedit Name="EulaRichedit" X="10" Y="10" Width="-10" Height="20" FontId="0"/>
        <Hyperlink Name="EulaHyperlink" X="10" Y="90" Width="-10" Height="24" FontId="1" HoverFontId="2" SelectedFontId="3">Click here for End User License Agreement</Hyperlink>
        <Checkbox Name="EulaAcceptCheckbox" X="10" Y="65" Width="200" Height="20" FontId="4">Accept terms of use.</Checkbox>
        <Button Name="WelcomeCancelButton" X="-10" Y="-10" Width="60" Height="24" FontId="0">Close</Button>
    </Page>
    <Page Name="Progress">
        <Button Name="ProgressCancelButton" X="40" Y="20" Width="200" Height="65" FontId="0">Close</Button>
    </Page>
    <Page Name="Modify">
        <Button Name="RepairButton" X="40" Y="20" Width="200" Height="65" FontId="0">Repair</Button>
        <Button Name="UninstallButton" X="40" Y="90" Width="200" Height="65" FontId="0">Uninstall</Button>
        <Button Name="ModifyCancelButton" X="-10" Y="-10" Width="90" Height="26" FontId="0">Close</Button>
    </Page>
    <Page Name="Finish">
        <Button Name="LaunchButton" X="40" Y="20" Width="200" Height="65" FontId="0" HideWhenDisabled="yes">Launch</Button>
        <Button Name="FinishCancelButton" X="40" Y="90" Width="200" Height="65" FontId="0">Close</Button>
    </Page>
    <Page Name="Failure">
        <Button Name="FailureCancelButton" X="40" Y="20" Width="200" Height="65" FontId="0">Close</Button>
    </Page>
</Theme>
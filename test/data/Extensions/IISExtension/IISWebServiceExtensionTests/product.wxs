          <Component Id="TestWebSvcExtProductComponent1" Guid="01AC7768-6B65-43d4-9114-F931FB25325D" DiskId="1">
            <File Source="$(env.WIX_ROOT)\test\data\SharedData\Files\TextFile1.txt"/>

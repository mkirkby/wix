          <Component Id="TestWebSvcExtProductComponent2" Guid="DD62DD5C-6CF3-4624-8ACB-101769315F06" DiskId="1">
            <File Id="TestFile2" Name="test2.exe" KeyPath="yes" Source="$(env.WIX_ROOT)\test\data\SharedData\Files\TextFile1.txt"/>

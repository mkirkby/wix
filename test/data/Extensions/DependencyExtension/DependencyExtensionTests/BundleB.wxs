<?ifndef Version?>
<?define Version = 1.0.0.0?>
<?endif?>

<?ifndef TestName?>
<?define TestName = Test?>
<?endif?>

<Wix xmlns="http://schemas.microsoft.com/wix/2006/wi" xmlns:dep="http://schemas.microsoft.com/wix/DependencyExtension">
    <Bundle Name="$(var.TestName) - Bundle B" Version="$(var.Version)" dep:ProviderKey="Microsoft.WiX.$(var.TestName).B">
        <BootstrapperApplicationRef Id="WixStandardBootstrapperApplication.HyperlinkLicense"/>
        <WixVariable Id="WixStdbaLicenseUrl" Value="http://wix"/>
        <Chain>
            <MsiPackage Compressed="yes" SourceFile="!(bindpath.packageA)" Vital="yes"/>
            <MsiPackage Compressed="yes" SourceFile="!(bindpath.PackageB)" Vital="yes"/>
        </Chain>
    </Bundle>
</Wix>

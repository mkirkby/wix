<Wix xmlns="http://schemas.microsoft.com/wix/2006/wi">

  <?define TestDataDirectory="$(env.WIX)\test\data\Integration\BuildingPackages\Bundle\Files"?>

  <Bundle>

    <UX Name="Setup.exe" SourceFile="$(var.TestDataDirectory)\Bootstrapper.exe">
    </UX>

    <Chain>
      <MsiPackage SourceFile="$(var.TestDataDirectory)\Packages\MsiPackage.msi" Vital="true"/>
      <MspPackage SourceFile="$(var.TestDataDirectory)\Packages\MspPackage.msp" Vital="false"/>
      <MsuPackage SourceFile="$(var.TestDataDirectory)\Packages\MsuPackage.msu" Vital="vital"/>
      <ExePackage SourceFile="$(var.TestDataDirectory)\Packages\ExePackage.exe" Vital="TRUE"/>
    </Chain>

  </Bundle>
</Wix>

<Wix xmlns="http://schemas.microsoft.com/wix/2006/wi" xmlns:util="http://schemas.microsoft.com/wix/UtilExtension">

  <?define TestDataDirectory="$(env.WIX)\test\data\Integration\BuildingPackages\Bundle\Files"?>

  <Bundle>

    <UX Name="Setup.exe" SourceFile="$(var.TestDataDirectory)\Bootstrapper.exe"/>

    <Chain>
      <MsiPackage SourceFile="$(var.TestDataDirectory)\Packages\MsiPackage.msi" After="UndefinedMsiPackage"/>
      <MspPackage SourceFile="$(var.TestDataDirectory)\Packages\MspPackage.msp" After="UndefinedMspPackage"/>
      <MsuPackage SourceFile="$(var.TestDataDirectory)\Packages\MsuPackage.msu" After="UndefinedMsuPackage"/>
      <ExePackage SourceFile="$(var.TestDataDirectory)\Packages\ExePackage.exe" After="UndefinedExePackage"/>
    </Chain>

  </Bundle>
</Wix>

<Wix xmlns="http://schemas.microsoft.com/wix/2006/wi" xmlns:dep="http://schemas.microsoft.com/wix/DependencyExtension">
    <Bundle Name="Various" Version="1.0.0.0" IconSourceFile="Setup.ico" Compressed="yes">
        <BootstrapperApplicationRef Id="ManagedBootstrapperApplicationHost">
            <Payload Name="BootstrapperCore.config" SourceFile="$(var.TestUX.TargetDir)\TestUX.BootstrapperCore.config"/>
            <Payload SourceFile="$(var.TestUX.TargetPath)"/>
            <Payload SourceFile="NetfxLicense.rtf"/>
        </BootstrapperApplicationRef>
        <Chain>
            <PackageGroupRef Id="Netfx4Full"/>
            <MsiPackage SourceFile="Products\Hello_World\hello_world.msi" Vital="yes"/>
            <MsiPackage SourceFile="MSIsandMSPs\RtmProduct\product.msi" Vital="yes">
                <MsiProperty Name="ARPSYSTEMCOMPONENT" Value="1"/>
                <dep:Provides Key="Microsoft.WiX.Tests.RTM,v1"/>
            </MsiPackage>
            <MspPackage SourceFile="MSIsandMSPs\GDR1\gdr1.msp" Vital="yes"/>
            <MspPackage SourceFile="MSIsandMSPs\LDR2\ldr2.msp" Vital="yes">
                <dep:Provides Key="Microsoft.WiX.Tests.LDR2,v2"/>
            </MspPackage>
            <ExePackage Id="TestExe1" SourceFile="Products\TestExe\TestExe.exe" InstallCommand="/s 5000" Vital="yes"/>
            <ExePackage Id="TestExe2" SourceFile="Products\TestExe\TestExe.exe" InstallCommand="/sr 5000-10000" Vital="yes">
                <dep:Provides Key="Microsoft.WiX.Tests.TestExe2,v1">
                    <dep:Requires ProviderKey="Microsoft.WiX.Tests.RTM,v1"/>
                </dep:Provides>
            </ExePackage>
        </Chain>
    </Bundle>
</Wix>
